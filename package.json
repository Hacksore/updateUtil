{
	"name": "macos-update",
	"version": "1.2.7",
	"description": "An Electron app that will help IT push macOS updates to non-admin users",
	"homepage": "https://github.com/Hacksore/updateUtil",
	"author": {
		"name": "Sean Boult",
		"email": "hacksore@mcskinsearch.com"
	},
	"keywords": [
		"macOS",
		"startosinstall",
		"update",
		"automation",
		"High Sierra"
	],
	"main": "main.js",
	"private": true,
	"scripts": {
		"postinstall": "npx electron-builder install-app-deps",
		"ng": "ng",
		"start": "node hooks/environments/set_profile.js && npm-run-all -p ng:serve electron:serve",
		"build": "node hooks/environments/set_profile.js && ng build && npm run electron:tsc",
		"build:prod": "node hooks/environments/set_profile.js && ng build --prod && npm run electron:tsc",
		"ng:serve": "ng serve",
		"electron:tsc": "tsc main.ts",
		"build:daemon": "tsc daemonHandler.ts && pkg daemonHandler.js --targets node9-macos-x64",
		"add:daemon": "cp daemonHandler 'app-builds/mac/macOS Update.app/Contents/Resources/'",
		"electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
		"electron:local": "npm run build:prod && electron .",
		"buildapp": "npm run build:prod && npx electron-builder build --mac && npm run build:daemon && npm run add:daemon",
		"test": "karma start ./karma.conf.js"
	},
	"dependencies": {
		"@angular/core": "5.2.9",
		"moment": "^2.22.1"
	},
	"devDependencies": {
		"@angular/cli": "1.7.4",
		"@angular/common": "5.2.9",
		"@angular/compiler": "5.2.9",
		"@angular/compiler-cli": "5.2.9",
		"@angular/core": "5.2.9",
		"@angular/forms": "5.2.9",
		"@angular/http": "5.2.9",
		"@angular/language-service": "5.2.9",
		"@angular/platform-browser": "5.2.9",
		"@angular/platform-browser-dynamic": "5.2.9",
		"@angular/router": "5.2.9",
		"@ngx-translate/core": "9.1.1",
		"@ngx-translate/http-loader": "2.0.1",
		"@types/core-js": "0.9.36",
		"@types/jasmine": "2.8.6",
		"@types/jasminewd2": "2.0.3",
		"@types/node": "7.0.7",
		"codelyzer": "4.2.1",
		"core-js": "2.5.5",
		"cross-env": "5.1.4",
		"dotenv": "5.0.1",
		"electron": "2.0.0",
		"electron-builder": "20.8.1",
		"electron-reload": "1.2.2",
		"fs-extra": "5.0.0",
		"jasmine-core": "2.99.1",
		"jasmine-spec-reporter": "4.2.1",
		"karma": "2.0.0",
		"karma-chrome-launcher": "2.2.0",
		"karma-coverage-istanbul-reporter": "1.4.2",
		"karma-jasmine": "1.1.1",
		"karma-jasmine-html-reporter": "0.2.2",
		"npm-run-all": "4.1.2",
		"npx": "10.0.1",
		"protractor": "5.3.1",
		"replace": "0.3.0",
		"rxjs": "5.5.8",
		"ts-node": "4.1.0",
		"tslint": "5.9.1",
		"typescript": "2.6.2",
		"wait-on": "2.1.0",
		"webdriver-manager": "12.0.6",
		"zone.js": "0.8.26"
	}
}